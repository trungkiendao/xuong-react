<h1>Product List</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">id</th>
      <th scope="col">Tên sản phẩm</th>
      <th scope="col">Giá</th>
    </tr>
  </thead>
  <tbody>


    {{#each data}}
    <tr>
      <th scope="row">{{id}}</th>
      <td>{{name}}</td>
      <td>{{price}}</td>
    </tr>
    {{/each}}



  </tbody>
</table>